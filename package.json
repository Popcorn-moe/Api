  },
  "lint-staged": {
    "*.{js,graphql}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Popcorn-moe/Popcorn.moe-Backend.git"
  },
  "author": "Popcorn-moe",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Popcorn-moe/Popcorn.moe-Backend/issues"
  },
  "homepage": "https://github.com/Popcorn-moe/Popcorn.moe-Backend#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^6.1.1",
    "prettier": "^1.10.2"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "apollo-upload-server": "^4.0.2",
    "body-parser": "^1.18.2",
