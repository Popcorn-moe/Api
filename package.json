    "graphql-iso-date": "^3.5.0",
    "graphql-subscriptions": "^0.5.7",
    "graphql-tools": "^2.23.1",
    "md5": "^2.2.1",
    "memoizee": "^0.4.11",
    "mime": "^1.6.0",
    "minio": "^4.0.0",
    "mongodb": "^2.2.34",
    "morgan": "^1.9.0",
    "node-dogstatsd": "0.0.6",
    "passport": "^0.4.0",
    "passport-anonymous": "^1.0.1",
    "passport-jwt": "^3.0.1",
    "sharp": "^0.19.1",
    "subscriptions-transport-sse": "^0.7.11"
  }
