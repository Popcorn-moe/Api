{
  "name": "popcorn.moe-backend",
  "version": "1.0.0",
  "description": "Popcorn.moe Backend",
  "main": "index.js",
  "scripts": {
    "start": "babel-node index.js",
    "test": "jest --collectCoverageFrom='src/**'",
